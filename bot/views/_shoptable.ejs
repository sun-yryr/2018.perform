<% for(let shopname in data) {%>
    <div class="w3-half" style="display: block;">
        <img class="w3-margin w3-circle" src="https://avatars0.githubusercontent.com/u/28134110?s=200&v=4">
        <table class="w3-table w3-white" style="width: 250px; display: block" id="search_shop_table">
            <p>class&nbsp;<%= shopname %></p>
            <tr>
                <th>item</th>
                <th>price</th>
            </tr>
            <%  for( let item in data[shopname].goods ) { %>
            <tr>
                    <td><%= item %></td>
                    <td><%= data[shopname].goods[item] %></td>
            </tr>
            <% }; %>
        </table>
    </div>
<% } %>